{
  "name": "urql-docs",
  "version": "1.0.0",
  "description": "Documentation site for urql",
  "private": true,
  "scripts": {
    "start": "react-static start",
    "build": "react-static build",
    "lint": "eslint --ext=js,jsx .",
    "clean": "rimraf dist",
    "prepublishOnly": "run-s clean build",
    "stage:build": "react-static build --staging",
    "prod:build": "react-static build",
    "prod:serve": "formideploy serve",
    "deploy": "formideploy deploy"
  },
  "babel": {
    "presets": [
      "react-static/babel-preset"
    ],
    "plugins": [
      "babel-plugin-styled-components"
    ]
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "../../scripts/eslint/js-preset.js"
    ]
  },
  "eslintIgnore": [
    "dist-prod"
  ],
  "dependencies": {
    "@mdx-js/react": "^1.6.22",
    "formidable-oss-badges": "0.3.5",
    "fuse.js": "^6.4.6",
    "history": "^4.7.2",
    "path": "^0.12.7",
    "preact": "^10.5.13",
    "prism-react-renderer": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-ga": "^3.3.0",
    "react-gtm-module": "^2.0.11",
    "react-inlinesvg": "^1.2.0",
    "react-is": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-ga": "^1.2.3",
    "react-scroll": "^1.8.1",
    "react-static": "7.2.2",
    "react-static-plugin-md-pages": "^0.3.2",
    "styled-components": "^5.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.0",
    "@mdx-js/mdx": "^1.5.7",
    "formideploy": "^0.4.1",
    "lodash": "^4.17.19",
    "react-hot-loader": "^4.12.20",
    "react-static-plugin-sitemap": "7.2.2",
    "react-static-plugin-styled-components": "7.2.2",
    "surge": "^0.21.3"
  }
}
